{
  "name": "document-creator-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@govtechsg/address-identity-resolver": "^1.4.1",
    "@govtechsg/decentralized-renderer-react-components": "^3.5.0",
    "@govtechsg/document-store": "^2.2.1",
    "@govtechsg/ethers-contract-hook": "^2.2.0",
    "@govtechsg/open-attestation": "5.2.2",
    "@govtechsg/token-registry": "^2.5.2",
    "@govtechsg/tradetrust-ui-components": "^1.10.1",
    "@hapi/joi": "^17.1.1",
    "@opengsn/gsn": "^2.1.0",
    "ajv": "^8.6.0",
    "axios": "^0.21.1",
    "csvtojson": "^2.0.10",
    "debug": "^4.3.1",
    "ethers": "^5.3.0",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "lodash": "^4.17.21",
    "pretty-bytes": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-feather": "^2.0.9",
    "react-jsonschema-form": "^1.8.1",
    "react-router-dom": "^5.2.0",
    "twin.macro": "^1.12.1",
    "use-persisted-state": "^0.3.0",
    "web3-providers-http": "^1.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/prompt": "^12.1.4",
    "@govtechsg/open-attestation-cli": "^1.40.4",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.13.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/debug": "^4.1.5",
    "@types/file-saver": "^2.0.2",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.23",
    "@types/mocha": "file:./src/types/empty-type",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.10",
    "@types/react-dom": "^17.0.7",
    "@types/react-jsonschema-form": "^1.7.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/use-persisted-state": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "commitizen": "^4.2.4",
    "concurrently": "^5.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^4.6.0",
    "ganache-cli": "^6.9.1",
    "git-cz": "^4.3.1",
    "http-server": "^0.12.3",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.1",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^17.0.2",
    "tailwindcss": "^1.9.6",
    "testcafe": "^1.14.2",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=4096' CI=false npm-run-all -s build:css build:js",
    "build:css": "NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "build:js": "NODE_ENV=production react-app-rewired build",
    "commit": "git-cz",
    "commit:retry": "npm run commit -- --retry",
    "eject": "react-scripts eject",
    "blockchain": "ganache-cli --mnemonic \"indicate swing place chair flight used hammer soon photo region volume shuffle\"",
    "integration": "./scripts/integration.sh",
    "integration:single": "./scripts/integration-single.sh",
    "integration:headless": "HEADLESS=true ./scripts/integration.sh",
    "integration:internal": "testcafe chrome integration/**/*.spec.ts --compiler-options typescript.configPath=./tsconfig.testcafe.json --app \"BROWSER=none npm run serve-static\"",
    "integration:internal:single": "testcafe chrome -L --compiler-options typescript.configPath=./tsconfig.testcafe.json --app \"npm run serve-static\"",
    "integration:internal:headless": "testcafe -c 4 chrome:headless integration/**/*.spec.ts --compiler-options typescript.configPath=./tsconfig.testcafe.json --app \"npm run serve-static\" --app-init-delay 70000",
    "integration:concurrently:headless": "concurrently -k -s first \"npm:blockchain\" \"npm:integration:headless\"",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:type": "npx --no-install tsc --noEmit --sourceMap false",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "patch-package",
    "serve-static": "http-server build -s -p 3000",
    "start": "npm-run-all -p start:*",
    "start:css": "postcss src/tailwind.css -o src/index.css --watch",
    "start:js": "sleep 2 && react-app-rewired start",
    "storybook": "npm-run-all -p start:css storybook:start",
    "storybook:start": "sleep 2 && start-storybook -p 9009 -s public",
    "storybook:build": "npm run build:css && build-storybook -s public",
    "prep": "npm-run-all -s lint lint:type lint:fix test:ci build integration:concurrently:headless",
    "test:ci": "CI=true react-app-rewired test --env=jest-environment-jsdom-sixteen",
    "test": "react-app-rewired test --env=jest-environment-jsdom-sixteen"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:import/typescript",
      "prettier",
      "plugin:testing-library/react"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "plugins": [
      "import",
      "react",
      "@typescript-eslint",
      "react-hooks",
      "testing-library"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/self-closing-comp": "error",
      "array-callback-return": "error",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "no-unused-expressions": "off",
      "jest/no-disabled-tests": "error",
      "no-shadow": "off",
      "testing-library/no-debug": [
        "error",
        {
          "utilsToCheckFor": {
            "debug": false,
            "logRoles": true,
            "logDOM": true
          }
        }
      ],
      "@typescript-eslint/explicit-function-return-type": [
        "warn",
        {
          "allowExpressions": true,
          "allowTypedFunctionExpressions": true
        }
      ],
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/no-use-before-define": 0
    },
    "overrides": [
      {
        "files": [
          "./**/*.js"
        ],
        "rules": {
          "@typescript-eslint/no-var-requires": "off"
        }
      },
      {
        "files": [
          "./**/*.stories.tsx"
        ],
        "rules": {
          "import/no-default-export": "off",
          "@typescript-eslint/explicit-function-return-type": "off",
          "@typescript-eslint/no-explicit-any": "off"
        }
      },
      {
        "files": [
          "*.test.ts",
          "*.test.tsx"
        ],
        "extends": [
          "plugin:jest/all"
        ],
        "rules": {
          "jest/prefer-expect-assertions": "off",
          "jest/no-hooks": "off",
          "jest/unbound-method": "off",
          "@typescript-eslint/unbound-method": "off",
          "@typescript-eslint/no-empty-function": "off",
          "@typescript-eslint/no-explicit-any": "off"
        }
      },
      {
        "files": [
          "*.spec.ts",
          "*.spec.tsx"
        ],
        "plugins": [
          "testcafe"
        ],
        "extends": "plugin:testcafe/recommended"
      }
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TradeTrust/document-creator-website.git"
  }
}
